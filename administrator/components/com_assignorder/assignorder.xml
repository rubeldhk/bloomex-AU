	<install>
		<queries>
			<query>
				CREATE TABLE IF NOT EXISTS `jos_postcode_warehouse` (
					  `id` int(11) NOT NULL auto_increment,
					  `postal_code` varchar(255) NOT NULL,
					  `warehouse_id` int(11) NOT NULL,
					  PRIMARY KEY  (`id`)
				) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;
			</query>
			<query>
				INSERT INTO `jos_postcode_warehouse` (`id`, `postal_code`, `warehouse_id`) VALUES
				(1, 'vvv', 8),
				(2, 'ttt', 7),
				(3, 'cop', 10);
			</query>
		</queries>
	</install>
	<uninstall>
		<queries>
			<query>DELETE FROM `jos_postcode_warehouse`;</query>
		</queries>
	</uninstall>
	
